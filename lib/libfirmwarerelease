copy_firmware()
{
	pushd $RELEASE_DIR
	cp ${PRODUCTDIR}/release/package/${PRODUCTVER}/${PRODUCTVER}.flash.* $RELEASE_DIR
	md5sum $PRODUCTVER.flash.pkg $PRODUCTVER.flash.rom > $PRODUCTVER.md5
	popd 
}

copy_history ()
{
	cp $OUTPUT_DIR/history.txt $RELEASE_DIR
}

copy_filesystem_log ()
{
	cp $PRODUCTDIR/release/log/* $RELEASE_DIR
}
