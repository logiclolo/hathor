copy_firmware()
{
	pushd $RELEASE_DIR
	cp ${PRODUCTDIR}/release/package/${PRODUCTVER}/${PRODUCTVER}.flash.* .
	md5sum $PRODUCTVER.flash.pkg $PRODUCTVER.flash.rom > $PRODUCTVER.md5
	popd 
}

copy_history ()
{
	pushd $RELEASE_DIR
	echo $OUTPUT_DIR
	cp $OUTPUT_DIR/history.txt .
}

copy_filesystem_log ()
{
	cp $PRODUCTDIR/release/log/* . 
}
