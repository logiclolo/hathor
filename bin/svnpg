#!/bin/sh

# svn st for common fw

IFS=$'\n'

svn_wc=`find $1 -iname ".svn" -type d`
#st_list="/tmp/svn-st.$$"

#svn st >> $st_list
for i in $svn_wc; do
	#svn st `dirname $i` >> $st_list
	if [ "$i" != "./.svn" ]; then
		svn pg svn:externals `dirname $i` -R
	fi
done

#cat $st_list | sort | uniq
#rm -f $st_list

